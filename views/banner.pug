div(class='slideshow')
    each val, i in [ "1.jpg","2.jpg" ]
        div(class='mySlides fade')
            div(class='numbertext') #{i}
            img(src=val, style='width:100%')
            div(class='text') Caption
br
div(style='text-align:center')
    each val, i in [ "1.jpg","2.jpg" ]
        span(class='dot',onclick='currentSlide('+i+')')